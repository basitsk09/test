app
    .controller('SC9Acontroller', function ($scope, $rootScope, $http, $window, $sessionStorage, $state, $timeout, $location, adminReportFactory, Idle, Keepalive, $modal, sc9aFactory, ModalService, blockUI, userFactory, refreshFactory, AES256) {

        if ($rootScope.reportObject == undefined) {
            refreshFactory.backToState();
            return;
        }

        $scope.sessionUser = JSON.parse(window.sessionStorage
            .getItem("user"));

        $scope.sessionUser = JSON.parse(AES256.decrypt($rootScope.globals.currentUser));
        var SC9A = this;
        $scope.started = false;
        SC9A.listOfRows = [];
        console.log("Report status is " + $rootScope.reportObject.status);

        if ($rootScope.reportObject.status != null || $rootScope.reportObject.status != undefined) {

            var circleCode = $scope.sessionUser.circleCode;
            var quarterEndDate = $scope.sessionUser.quarterEndDate;

            var row1 = {
                'circleCode': circleCode,
                'quarterEndDate': quarterEndDate,
                'userId': $scope.sessionUser.userId,
                'reportName': $rootScope.reportObject.name,
                'reportId': $rootScope.reportObject.reportId,
                'reportMasterId': $rootScope.reportObject.reportMasterId,
                'status': $rootScope.reportObject.status,
                'areMocPending': $rootScope.reportObject.areMocPending
            };

            sc9aFactory
                .getSavedDataNineA(row1)
                .then(function (data) {
                    console.log("editing");
                    var list = [];
                    list = data;
                    SC9A.listOfRows = data;

                    //console.log(SC9A.listOfRows);


                }, function (errResponse) {
                    console
                        .error('Error getting saved data in schedule 9A');
                });

        }

        // //////////////////////submit

        SC9A.submitNineA = function (saveVal) {

            var valid = true;

            SC9A.save = saveVal;
            console.log("**********is for saving data ? " + SC9A.save);


            for (var i = 0; i < SC9A.listOfRows.length; i++) {
                var borrowerName = SC9A.listOfRows[i].borrowerName;
                var aggOutStand = SC9A.listOfRows[i].aggOutStand;
                var aggSecurities = SC9A.listOfRows[i].aggSecurities;
                var netShortfall = SC9A.listOfRows[i].netShortfall;
                var provision = SC9A.listOfRows[i].provision;
                var balInterestSuspenseAcc = SC9A.listOfRows[i].balInterestSuspenseAcc;

                /*console.log("borrowerName "+borrowerName);
                    console.log("aggOutStand "+SC9A.parseFloat(aggOutStand));
                    console.log("aggSecurities "+SC9A.parseFloat(aggSecurities));
                    console.log("netShortfall "+SC9A.parseFloat(netShortfall));
                    console.log("provision "+SC9A.parseFloat(provision));
                    console.log("balInterestSuspenseAcc "+SC9A.parseFloat(balInterestSuspenseAcc));*/

                /*if (SC9A.save==true){
                        if ((borrowerName == ''||borrowerName == undefined) && (aggOutStand == '' ||aggOutStand == undefined)
                            && (aggSecurities == '' || aggSecurities == undefined)
                            && (netShortfall == '' || netShortfall == undefined)
                            && (provision == '' || provision == undefined)
                            && (balInterestSuspenseAcc == '' || balInterestSuspenseAcc == undefined)) {

                            console.log('validation condition  failed while save');
                            valid = false;

                        }
                    }
                    if (SC9A.save==false){

                        if (borrowerName == '' && SC9A.parseFloat(aggOutStand) == 0
                            && SC9A.parseFloat(aggSecurities) == 0
                            && SC9A.parseFloat(netShortfall) == 0
                            && SC9A.parseFloat(provision) == 0
                            && SC9A.parseFloat(balInterestSuspenseAcc) == 0) {

                            console.log('validation condition  failed while submit');
                            valid = false;

                        }

                    }*/
                if ((borrowerName == '' || borrowerName == null) && SC9A.parseFloat(aggOutStand) == 0 && SC9A.parseFloat(aggSecurities) == 0 && SC9A.parseFloat(netShortfall) == 0 && SC9A.parseFloat(provision) == 0 && SC9A.parseFloat(balInterestSuspenseAcc) == 0) {

                    console.log('validation condition  failed while submit/save');
                    valid = false;

                }


            }

            console.log("valid ? " + valid);

            if (valid == true) {
                if ($scope.sc9AForm.$dirty == false) {

                    if (SC9A.save == false) {
                        SC9A.displayMessage111 = "Would you like to submit report without changing data ?";
                    }
                    if (SC9A.save == true) {
                        SC9A.displayMessage111 = "Would you like to save report without changing data ?";
                    }

                    if (SC9A.displayMessage111) {

                        $('#myModal3').modal({
                            backdrop: 'static', keyboard: false, modal: true
                        });
                        $('#myModal3')
                            .on('shown.bs.modal', function () {
                                $('#myModal3').trigger('focus');
                            });
                    }

                }

                SC9A.YesSubmitSC9A = function () {

                    var circleCode = $scope.sessionUser.circleCode;
                    var quarterEndDate = $scope.sessionUser.quarterEndDate;
                    var listOfSC9A = SC9A.listOfRows;
                    var listToBeSent = [];

                    listOfSC9A
                        .forEach(function (s) {

                            var sc9aObject = {

                                'borrowerName': s.borrowerName,
                                'aggOutStand': s.aggOutStand,
                                'aggSecurities': s.aggSecurities,
                                'netShortfall': s.netShortfall,
                                'provision': s.provision,
                                'balInterestSuspenseAcc': s.balInterestSuspenseAcc

                            };
                            listToBeSent.push(sc9aObject);
                        });

                    var rowSubmit = {
                        'listToBeSent': listToBeSent,
                        'circleCode': circleCode,
                        'quarterEndDate': quarterEndDate,
                        'userId': $scope.sessionUser.userId,
                        'reportName': $rootScope.reportObject.name,
                        'reportId': $rootScope.reportObject.reportId,
                        'reportMasterId': $rootScope.reportObject.reportMasterId,
                        'status': $rootScope.reportObject.status,
                        'save': SC9A.save
                    };

                    // var
                    // params={'listToBeSent':listToBeSent,'circleCode':$scope.sessionUser.circleCode,'quarterEndDate':$scope.sessionUser.quarterEndDate,'level':$scope.level,'userId':$scope.sessionUser.userId};
                    console.log('params ' + rowSubmit);

                    console.log("************** SC 9A ********* save ? " + SC9A.save);
                    sc9aFactory
                        .submitNineA(rowSubmit)
                        .then(function (data) {


                            console.log("************** submitStatus ~ Sequence (reportId) ~ reportStatus : " + data);
                            var result = [];
                            result = data.split('~');


                            if (result[0] >= 0) {

                                $rootScope.reportObject.reportId = result[1];
                                $rootScope.reportObject.status = result[2];
                                console.log("# reportId " + $rootScope.reportObject.reportId);
                                console.log("# status " + $rootScope.reportObject.status);


                                if (SC9A.save == false) {
                                    SC9A.displayMessage = "Report Submitted Successfully";
                                    if (SC9A.displayMessage) {

                                        $('#myModal1')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });
                                        $('#myModal1')
                                            .on('shown.bs.modal', function () {
                                                $('#myModal1')
                                                    .trigger('focus');
                                            });
                                    }
                                }
                                if (SC9A.save == true) {
                                    SC9A.displayMessage = "Report Saved Successfully";
                                    if (SC9A.displayMessage) {

                                        $('#myModal4')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });
                                        $('#myModal4')
                                            .on('shown.bs.modal', function () {
                                                $('#myModal4')
                                                    .trigger('focus');
                                            });
                                    }
                                }


                            }
                        }, function (errResponse) {
                            console
                                .error('Error while submitting schedule 9A');
                        });

                }
                // ///////////////////////////////////////////////////

                if ($scope.sc9AForm.$dirty == true) {

                    // /////////////////////////old
                    console.log(SC9A.listOfRows);
                    var circleCode = $scope.sessionUser.circleCode;
                    var quarterEndDate = $scope.sessionUser.quarterEndDate;
                    var listOfSC9A = SC9A.listOfRows;
                    var listToBeSent = [];

                    listOfSC9A
                        .forEach(function (s) {
                            console.log(s);

                            var sc9aObject = {

                                'borrowerName': s.borrowerName,
                                'aggOutStand': s.aggOutStand,
                                'aggSecurities': s.aggSecurities,
                                'netShortfall': s.netShortfall,
                                'provision': s.provision,
                                'balInterestSuspenseAcc': s.balInterestSuspenseAcc

                            };
                            listToBeSent.push(sc9aObject);
                        });

                    var rowSubmit = {
                        'listToBeSent': listToBeSent,
                        'circleCode': circleCode,
                        'quarterEndDate': quarterEndDate,
                        'userId': $scope.sessionUser.userId,
                        'reportName': $rootScope.reportObject.name,
                        'reportId': $rootScope.reportObject.reportId,
                        'reportMasterId': $rootScope.reportObject.reportMasterId,
                        'status': $rootScope.reportObject.status,
                        'save': SC9A.save
                    };

                    console.log("*********************** is save ?" + SC9A.save);
                    // var
                    // params={'listToBeSent':listToBeSent,'circleCode':$scope.sessionUser.circleCode,'quarterEndDate':$scope.sessionUser.quarterEndDate,'level':$scope.level,'userId':$scope.sessionUser.userId};
                    console.log('params ' + rowSubmit);

                    sc9aFactory
                        .submitNineA(rowSubmit)
                        .then(function (data) {

                            console.log("************** submitStatus ~ Sequence (reportId) ~ reportStatus : " + data);
                            var result = [];
                            result = data.split('~');


                            if (result[0] >= 0) {

                                $rootScope.reportObject.reportId = result[1];
                                $rootScope.reportObject.status = result[2];
                                console.log("# reportId " + $rootScope.reportObject.reportId);
                                console.log("# status " + $rootScope.reportObject.status);


                                if (SC9A.save == false) {
                                    SC9A.displayMessage = "Report Submitted Successfully";
                                    if (SC9A.displayMessage) {

                                        $('#myModal1')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });
                                        $('#myModal1')
                                            .on('shown.bs.modal', function () {
                                                $('#myModal1')
                                                    .trigger('focus');
                                            });
                                    }
                                }
                                if (SC9A.save == true) {
                                    SC9A.displayMessage = "Report Saved Successfully";
                                    if (SC9A.displayMessage) {

                                        $('#myModal4')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });
                                        $('#myModal4')
                                            .on('shown.bs.modal', function () {
                                                $('#myModal4')
                                                    .trigger('focus');
                                            });
                                    }
                                }

                            }
                            console.log(data);
                        }, function (errResponse) {
                            console
                                .error('Error while submtting sc 9A');
                        });
                    // //////////old

                }

                // ///////valid if end
            } else {
                SC9A.displayMessage = "please enter data, Empty rows can't be submitted.";

                if (SC9A.save == true) {
                    SC9A.displayMessage = "please enter data, Empty rows can't be saved.";
                }

                if (SC9A.displayMessage) {

                    $('#myModal4').modal({
                        backdrop: 'static', keyboard: false, modal: true
                    });
                    $('#myModal4').on('shown.bs.modal', function () {
                        $('#myModal4').trigger('focus');
                    });
                }
            }

        }

        SC9A.redirect = function () {
            $timeout(function () {
                $state.go('circle_maker.worklist');
            }, 500);
        }

        SC9A.parseFloat = function (value) {
            if (isNaN(value)) {
                value = 0;
            }
            return parseFloat(value * 1);
        }

        $scope.$watch('SC9A.listOfRows', function (newVal) {


            SC9A.aggOutStandTotal = 0;
            SC9A.aggSecuritiesTotal = 0;
            SC9A.netShortfallTotal = 0;
            SC9A.provisionTotal = 0;
            SC9A.balInterestSuspenseAccTotal = 0;

            if (newVal != undefined) {

                newVal.forEach(function (s) {

                    //console.log("QQQQQQQQQQQQQQQQQQQ "+JSON.stringify(newVal));
                    //console.log("******aggOutStand "+s.aggOutStand+" ********aggSecurities "+s.aggSecurities);


                    if (s.aggOutStand == '' || s.aggOutStand == undefined) {
                        s.aggOutStand = 0;
                    }

                    if (s.aggSecurities == '' || s.aggSecurities == undefined) {
                        s.aggSecurities = 0;
                    }


                    if (s.aggOutStand == '-' && s.aggOutStand.length == 1) {
                        s.netShortfall = (-1 * (SC9A.parseFloat(s.aggSecurities)).toFixed(2)).toString();
                    } else if (s.aggSecurities == '-' && s.aggSecurities.length == 1) {
                        s.netShortfall = ((SC9A.parseFloat(s.aggOutStand)).toFixed(2)).toString();
                    } else {
                        s.netShortfall = (SC9A.parseFloat(s.aggOutStand)).toFixed(2) - (SC9A.parseFloat(s.aggSecurities)).toFixed(2);

                        s.netShortfall = ((SC9A.parseFloat(s.netShortfall)).toFixed(2)).toString();

                    }

                    SC9A.aggOutStandTotal = (SC9A.parseFloat(SC9A.aggOutStandTotal) + SC9A.parseFloat(s.aggOutStand)).toFixed(2);
                    SC9A.aggSecuritiesTotal = (SC9A.parseFloat(SC9A.aggSecuritiesTotal) + SC9A.parseFloat(s.aggSecurities)).toFixed(2);
                    SC9A.netShortfallTotal = (SC9A.parseFloat(SC9A.netShortfallTotal) + SC9A.parseFloat(s.netShortfall)).toFixed(2);
                    SC9A.provisionTotal = (SC9A.parseFloat(SC9A.provisionTotal) + SC9A.parseFloat(s.provision)).toFixed(2);
                    SC9A.balInterestSuspenseAccTotal = (SC9A.parseFloat(SC9A.balInterestSuspenseAccTotal) + SC9A.parseFloat(s.balInterestSuspenseAcc)).toFixed(2);


                    s.aggOutStand = (s.aggOutStand).toString();
                    s.aggSecurities = (s.aggSecurities).toString();

                });


            }


        }, true);

        SC9A.addRow = function () {
            blockUI.start();
            SC9A.listOfRows.push({
                'isDelete': '',
                'borrowerName': "",
                'aggOutStand': SC9A.aggOutStand,
                'aggSecurities': SC9A.aggSecurities,
                'netShortfall': SC9A.netShortfall,
                'provision': "",
                'balInterestSuspenseAcc': ""
            });
            blockUI.stop();
        };

        SC9A.deleteRow = function () {
            var listOfRows = SC9A.listOfRows;
            SC9A.listOfRows.forEach(function (s) {
                if (s.isDelete == true) {
                    var index = SC9A.listOfRows.indexOf(s);
                    //console.log(index);
                    SC9A.listOfRows.splice(index, 1);
                    // /added on 15062018
                    SC9A.deleteRow();
                    SC9A.displayMessage = "Row deleted successfully, Deletion of row will refelect in the report after submitting the report.";

                    if (SC9A.displayMessage) {

                        $('#myModal4')
                            .modal({
                                backdrop: 'static', keyboard: false, modal: true
                            });
                        $('#myModal4')
                            .on('shown.bs.modal', function () {
                                $('#myModal4')
                                    .trigger('focus');
                            });
                    }
                    return;
                    // /added on 15062018 End
                }
            });
        }

    });

	 sc10Factory.getValidationDataTen(row1)
                .then(function (data) {
                    console.log("Inside getValidationDataTen");
                    console.log("JSOn Data Getting FOR SC10 getSC10DataRegular:" + JSON.stringify(data));
                    console
                        .log("OtherFixedAssetAmount " + data.validationOtherFixedAssetAmount);
                    console
                        .log("PremisesAmount " + data.validationPremisesAmount);
                    console
                        .log("PremisesUnderConsAmount " + data.validationPremisesUnderConsAmount);

                    var validPremisesAmount = data.validationPremisesAmount;
                    var validPremisesUnderConsAmount = data.validationPremisesUnderConsAmount;
                    var validOtherFixedAssetAmount = data.validationOtherFixedAssetAmount;
                    var rowValid = ({
                        'validPremisesAmount': validPremisesAmount,
                        'validPremisesUnderConsAmount': validPremisesUnderConsAmount,
                        'validOtherFixedAssetAmount': validOtherFixedAssetAmount
                    });

                    sc10.row.mdData1 = data.validationOtherFixedAssetAmount;
                    sc10.row.mdData2 = data.validationPremisesAmount;
                    sc10.row.mdData3 = data.validationPremisesUnderConsAmount;

                    sc10.row = angular.extend({}, sc10.row, rowValid);

                }

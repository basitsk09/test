                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column [LC]",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8"
                    });

            }

            if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8) == parseFloat(ysa.rowYsaData.ttl_JKGrnteCol4Row12))) {

                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column Guarantees",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8"
                    });

            }

            if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8) == parseFloat(ysa.rowYsaData.ttl_JKAcptCol6Row12))) {

                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column Acceptances.",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8"
                    });

            }

            if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8) == parseFloat(ysa.rowYsaData.ttl_JKLUCol8Row12))) {

                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column [LOC]",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8"
                    });

            }

            if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3))) {
                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column [LC]",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8"
                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3))) {
                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column Guarntee",
                        'BeanName': "ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8"
                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3))) {
                validateArray
                    .push({

            $scope.validateArray = validateArray;

            if (validateArray.length == 0) {
                alert("No Errors, You can submit report");
            } else {

                $scope.myvalue = true;

            }

        }

        ysa.GoToValidate = function (value) {

            var abc = document.getElementsByName(value)[0];

            $("#xyz a:first").tab('show');
            abc.focus();
            abc.style.border = "2px solid red";
            // abc.addClass("validateColor");

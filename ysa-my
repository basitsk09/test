                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlOutstndngInlndGrnteCol4Row1) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10) != 0.00) || ((ysa.rowYsaData.ttlOutstndngInlndGrnteCol4Row1) == null && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngInlndGrnteCol4Row1) == "" && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10) != 0.00))) {

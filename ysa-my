//Controller code



app
    .controller('YSAController', function ($scope, $rootScope, $http, $window, $sessionStorage, $state, $location, adminReportFactory, Idle, Keepalive, $modal, ModalService, userFactory, ysaFactory, refreshFactory, AES256) {
        if ($rootScope.reportObject == undefined) {
            refreshFactory.backToState();
            return;
        }
        $scope.sessionUser = JSON.parse(AES256.decrypt($rootScope.globals.currentUser));

        var ysa = this;

        ysa.qdate = $scope.sessionUser.quarterEndDate;


        var a1 = $scope.sessionUser.quarterEndDate;
        var quarter_end_date = $scope.sessionUser.quarterEndDate.toString().replace("/", ".");
        var previousYearEndDate = $scope.sessionUser.previousYearEndDate.toString().replace("/", ".");
        var quayear = $scope.sessionUser.quarter.toString();

        var b1 = $scope.sessionUser.previousQuarterEndDate.toString();


        var a2 = "";
        var a4 = "";
        var a5 = "";
        var a8 = "";
        var a6 = "";
        var a7 = "";
        var prevYear = "";
        var curYear = quarter_end_date.substring(6, 10);


        if (quayear == "Q1") {
            a2 = a1.replace("06", "th June ").replace("/", "");
            a4 = "QSA";
            ysa.a5 = "31st December,";
            a8 = "June";
            a6 = previousYearEndDate.substring(6, 10);
            ysa.a7 = previousYearEndDate.substring(6, 10);
            prevYear = quarter_end_date.substring(6, 10);
        }

        if (quayear == "Q2") {
            a2 = a1.replace("09", "th September ").replace("/", "");
            a4 = "QSA";
            ysa.a5 = "31st March,";
            a8 = "September";
            a6 = quarter_end_date.substring(6, 10);
            ysa.a7 = quarter_end_date.substring(6, 10);
            prevYear = quarter_end_date.substring(6, 10);
        }

        if (quayear == "Q3") {
            a2 = a1.replace("12", "st December ").replace("/", "");
            a4 = "QSA";
            ysa.a5 = "30th June,";
            a8 = "December";
            a6 = quarter_end_date.substring(6, 10);
            ysa.a7 = quarter_end_date.substring(6, 10);
            prevYear = quarter_end_date.substring(6, 10);
        }
        if (quayear == "Q4") {
            a2 = a1.replace("03", "st March").replace("/", " ");
            a4 = "YSA";
            ysa.a5 = "30th September,";
            a8 = "March";
            a6 = previousYearEndDate.substring(6, 10);
            ysa.a7 = b1.substring(6, 10);
            prevYear = previousYearEndDate.substring(6, 10);
        }


        var forCCode02301;
        var forCCode02302;
        var forCCode02303;
        var forCCode02842;
        var forCCode02843;
        var forCCode20601;
        var forCCode20602;

        var forCCode01027;

        var forCCode20471;
        var forCCode01867;
        var forCCode20167;
        var forCCode20199;

        $scope.started = false;
        var reportObject = $rootScope.reportObject;
        console.log('inside ysacontroller ' + JSON.stringify($rootScope.reportObject));

        ysa.getPreRequiAmount = {};

        var row = {
            'circleCode': $scope.sessionUser.circleCode, 'quarterEndDate': $scope.sessionUser.quarterEndDate
        };

        ysaFactory.getPreRequisteHeaderAmount(row).then(function (data) {

            ysa.getPreRequiAmount = data;
            console.log(data);
            var a, b, c, d, e, f, g, h, i, j, k, l;

            for (var keyName in data) {
                var key = keyName;
                var value = data[keyName];
                if (key == "02302") {
                    b = value;
                }
                if (key == "02303") {
                    c = value;
                }

                if (key == "02301") {
                    a = value;
                }

                if (key == "02843") {
                    e = value;
                }

                if (key == "02842") {
                    d = value;
                }

                if (key == "20601") {
                    f = value;
                }

                if (key == "20602") {
                    g = value;
                }

                if (key == "01027") {
                    h = value;
                }

                if (key == "20471") {
                    i = value;
                }

                if (key == "01867") {
                    j = value;
                }

                if (key == "20167") {
                    k = value;
                }

                if (key == "20199") {
                    l = value;
                }
            }

            forCCode02301 = a;
            forCCode02302 = b;
            forCCode02303 = c;
            forCCode02842 = d;
            forCCode02843 = e;
            forCCode20601 = f;
            forCCode20602 = g;

            forCCode01027 = h;

            forCCode20471 = i;
            forCCode01867 = j;
            forCCode20167 = k;
            forCCode20199 = l;

        }, function (errResponse) {
            console.error('Error while submit ysa');
        });

        ysa.PreCheckAmount = function () {

            $('#myModal55').modal({
                backdrop: 'static', keyboard: false, modal: true
            });

        }

        ysa.submitYSA = function (rowYsaData, save) {

            console.log("********* for saving data ? " + save);
            var i = {};
            i = ysa.rowYsaData;
            console.log(i);
            var circleCode = $scope.sessionUser.circleCode;
            var quarterEndDate = $scope.sessionUser.quarterEndDate;
            var row1 = {
                'circleCode': circleCode,
                'quarterEndDate': quarterEndDate,
                'userId': $scope.sessionUser.userId,
                'reportName': reportObject.name,
                'reportId': reportObject.reportId,
                'reportMasterId': reportObject.reportMasterId,
                'status': reportObject.status,
                'areMocPending': reportObject.areMocPending,
                'save': save
            };

            var copy = angular.extend({}, i, row1);

            var validateArray = [];
            var valid = true;

            // alert(ysa.rowYsaData.intrstFreStf_Advncs);

            if (ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI == null || ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI == "") {
                ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.intrstFreStf_Advncs == null || ysa.rowYsaData.intrstFreStf_Advncs == "") {
                ysa.rowYsaData.intrstFreStf_Advncs = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11 == null || ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11 == "") {
                ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11 == null || ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11 == "") {
                ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11 == null || ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11 == "") {
                ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLUCol8Row11 == null || ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLUCol8Row11 == "") {
                ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLUCol8Row11 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofRupTrnscnsLCCol2Row10 == null || ysa.rowYsaData.onAcntofRupTrnscnsLCCol2Row10 == "") {
                ysa.rowYsaData.onAcntofRupTrnscnsLCCol2Row10 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10 == null || ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10 == "") {
                ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10 == null || ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10 == "") {
                ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10 = (0.00)
                    .toFixed(2);

            }

            if (ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10 == null || ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10 == "") {
                ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10 = (0.00)
                    .toFixed(2);

            }

            //ignoring validations while saving data
            if (save == false) {

                if (!(parseFloat(ysa.rowYsaData.totalfortbl1) == ysa
                    .parseFloat(forCCode20471).toFixed(2))) {
                    validateArray
                        .push('- Break Up of Out Standings in Suspense Account (40049) should Match with YSA Comp Code (20471)');
                    valid = false;
                }

                if (!(parseFloat(ysa.rowYsaData.totalfortbl2a) == ysa
                    .parseFloat(forCCode01867).toFixed(2))) {
                    validateArray
                        .push('- Break Up of Out Standings in Sundry Deposits Account (40119) should Match with YSA Comp Code (01867)');
                    valid = false;
                }

                /*if (!(parseFloat(ysa.rowYsaData.intrstFreStf_Advncs) == (ysa
                        .parseFloat(forCCode20167) + ysa
                        .parseFloat(forCCode20199)).toFixed(2))) {
                        validateArray
                            .push('- Interest free Advance to Staff - Festival Advance (40211) should Match with YSA Comp Code [20167 + 20199]');
                        valid = false;
                    }*/

                /*
                             * if(!(parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3)==ysa.parseFloat(forCCode02302).toFixed(2)) ||
                             * !(ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3==ysa.parseFloat(forCCode02301).toFixed(2)) ||
                             * !(ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3==ysa.parseFloat(forCCode02303).toFixed(2))) {
                             * validateArray.push('- Total Outstanding (Inland +
                             * Foreign) should match from YSA CompCode 02302, 02301,
                             * 02303'); valid=false; }
                             */

                // ////////////////
                if (!(parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3) == ysa
                    .parseFloat(forCCode02302).toFixed(2))) {
                    validateArray
                        .push('- Total Outstanding (Inland + Foreign) for column (letter of credit)  should match from YSA CompCode 02302');

                }

                if (!(parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3) == ysa
                    .parseFloat(forCCode02301).toFixed(2))) {
                    validateArray
                        .push('- Total Outstanding (Inland + Foreign) for column (Guarantees) should match from YSA CompCode 02301');

                }

                if (!(parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3) == ysa
                    .parseFloat(forCCode02303).toFixed(2))) {
                    validateArray
                        .push('- Total Outstanding (Inland + Foreign) for column (Acceptances) should match from YSA CompCode 02303');
                }

                // ////////////////

                if (!(parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLUCol8Row3) == (ysa
                    .parseFloat(forCCode02842) + ysa
                    .parseFloat(forCCode02843)).toFixed(2))) {
                    validateArray
                        .push('- Total Outstanding (Inland + Foreign) for letter of undertaking should match from amount of CompCode (02842+02843)');
                    valid = false;
                }

                /*
                             * if(!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8)==ysa.rowYsaData.ttl_JKLCCol2Row12) ||
                             * !(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8==ysa.rowYsaData.ttl_JKGrnteCol4Row12) ||
                             * !(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8==ysa.rowYsaData.ttl_JKAcptCol6Row12) ||
                             * !(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8==ysa.rowYsaData.ttl_JKLUCol8Row12)) {
                             * validateArray.push('- [H] Total Value of Margin
                             * Money/Lien held and [L] Total of Break up must be
                             * match for each column.'); valid=false; }
                             */

                // ///////////////
                if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8) == parseFloat(ysa.rowYsaData.ttl_JKLCCol2Row12))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column [LC]');

                }

                if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8) == parseFloat(ysa.rowYsaData.ttl_JKGrnteCol4Row12))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column Guarantees');

                }

                if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8) == parseFloat(ysa.rowYsaData.ttl_JKAcptCol6Row12))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column Acceptances.');

                }

                if (!(parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8) == parseFloat(ysa.rowYsaData.ttl_JKLUCol8Row12))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held and [L] Total of Break up must be match for column [LOC]');

                }

                // //////////////////

                /*
                             * if(
                             * (parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8)>ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3) ||
                             * (ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8>ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3) ||
                             * (ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8>ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3) ||
                             * (ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8>ysa.rowYsaData.ttlOutstndng_InlndFrgnabLUCol8Row3)) {
                             * validateArray.push('- [H] Total Value of Margin
                             * Money/Lien held can not exceed [C] Total Outstanding
                             * (Inland + Foreign) for each column'); valid=false; }
                             */
                // //////////////////////////
                if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column [LC]');

                }

                if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column Guarntee');

                }

                if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column Acceptances');

                }

                if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLUCol8Row3))) {
                    validateArray
                        .push('- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column [LOC]');

                }

                // ////////////////////////

                // ///////// start ///////////////
                // /////1///////

                /* if (!(parseFloat(ysa.rowYsaData.subTtl_OnToThreForLCCol2Row5) == parseFloat(ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI))) {
                         validateArray
                             .push('- (40515) Sub-Total (assets-wise) and (40201) Bills Rediscounted  are not matching');

                     }*/

                // /////2///////

                /* if (!(parseFloat(ysa.rowYsaData.subTtl_Six_Svn_EghtLCCol2Row9) == parseFloat(ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI))) {
                         validateArray
                             .push('- (40535) Sub-Total (assets-wise) and (40201) Bills Rediscounted  are not matching');

                     }*/

                // /////3///////

                /* if (!(parseFloat(ysa.rowYsaData.subTtl_Ten_Elvn_Twlv_ThrtnLCCol2Row14) == parseFloat(ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI))) {
                         validateArray
                             .push('- (40560) Sub-Total (assets-wise) and (40201) Bills Rediscounted  are not matching');

                     }*/

                // ///////// end ///////////////

                /*

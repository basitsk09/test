                // / internal added on 20122017 ///

                if ((parseFloat(ysa.rowYsaData.ttlOutstndngFrgnLCCol2Row2) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11) != 0.00) || ((ysa.rowYsaData.ttlOutstndngFrgnLCCol2Row2) == null && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngFrgnLCCol2Row2) == "" && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11) != 0.00))) {
                    validateArray
                        .push('- LCs - [K] On account of Foreign Currency Transactions should be zero , if [B] Total Outstanding Foreign is zero.');

                }

                if ((parseFloat(ysa.rowYsaData.ttlOutstndngFrgnGrnteCol4Row2) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11) != 0.00) || ((ysa.rowYsaData.ttlOutstndngFrgnGrnteCol4Row2) == null && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngFrgnGrnteCol4Row2) == "" && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11) != 0.00))) {
                    validateArray
                        .push('- Guarantees - [K] On account of Foreign Currency Transactions should be zero , if [B] Total Outstanding Foreign is zero.');

                }

                if ((parseFloat(ysa.rowYsaData.ttlOutstndngFrgnAcptCol6Row2) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11) != 0.00) || ((ysa.rowYsaData.ttlOutstndngFrgnAcptCol6Row2) == null && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngFrgnAcptCol6Row2) == "" && (parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11) != 0.00))) {
                    validateArray
                        .push('-Acceptances - [K] On account of Foreign Currency Transactions should be zero , if [B] Total Outstanding Foreign is zero.');

                }


                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlOutstndngInlndAcptCol6Row1) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10) != 0.00) || ((ysa.rowYsaData.ttlOutstndngInlndAcptCol6Row1) == null && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngInlndAcptCol6Row1) == "" && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10) != 0.00))) {
                validateArray
                    .push({
                        'ErrorName': "Acceptances - [J] On account of Foreign Currency Transactions should be zero , if [A] Total Outstanding Foreign is zero. ",
                        'BeanName': "onAcntofRupTrnscnsAcptCol6Row10"
                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlOutstndngInlndLUCol8Row1) == 0.00 && parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10) != 0.00) || ((ysa.rowYsaData.ttlOutstndngInlndLUCol8Row1) == null && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10) != 0.00)) || ((ysa.rowYsaData.ttlOutstndngInlndLUCol8Row1) == "" && (parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10) != 0.00))) {

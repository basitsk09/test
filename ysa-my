        // var ysa=this;
        $scope.$watchCollection('ysa.rowYsaData', function (newValues, oldValues) {
            // alert("abcd");
            // alert(ysa.rowYsaData);
            if (ysa.rowYsaData != undefined) {
                ysa.calculateTotal();
            }
        });

        ysa.calculateTotal = function () {
            // alert("qwq "+ysa.rowYsaData.dmandDep);

            // part 1 //
            ysa.rowYsaData.ttlforstaffsecrty = (ysa
                .parseFloat(ysa.rowYsaData.dmandDep) + ysa
                .parseFloat(ysa.rowYsaData.timeDep)).toFixed(2);

            // part 2 //

            ysa.rowYsaData.advancesToStaffTA_HA_Medicalab = (ysa
                .parseFloat(ysa.rowYsaData.jrny_trtmentCompltdOnorBefore_thirtyfirst_March_fifteen) + ysa
                .parseFloat(ysa.rowYsaData.jrny_trtmentNotCompltdOn_thirtyfirst_March_fifteen))
                .toFixed(2);

            ysa.rowYsaData.totalfortbl1 = (ysa
                .parseFloat(ysa.rowYsaData.bonusPaidtoStaff_No_Outstandings) + ysa
                .parseFloat(ysa.rowYsaData.clearngEntriesOutstandings_Clear_Chq_Rtrnd) + ysa
                .parseFloat(ysa.rowYsaData.clearngDiffrncsPrkedinSuspenseAcnt_upto_Thirtyth_Sept_Fortn) + ysa
                .parseFloat(ysa.rowYsaData.clearngDiffrncsPrkedinSuspenseAcnt_after_Thirtyth_Sept_Fortn) + ysa
                .parseFloat(ysa.rowYsaData.entrsOutstndPriortoSweepinginSysSusAcnt98724) + ysa
                .parseFloat(ysa.rowYsaData.subsidyRecvbleUnderPMRYSchm) + ysa
                .parseFloat(ysa.rowYsaData.advcPymntsMadeforFixedAssts) + ysa
                .parseFloat(ysa.rowYsaData.dbtOnAccntofFraudOthrLos) + ysa.parseFloat(ysa.rowYsaData.lawCharges) + ysa.parseFloat(ysa.rowYsaData.courtFees) + ysa.parseFloat(ysa.rowYsaData.othersfrtbl1) + ysa
                .parseFloat(ysa.rowYsaData.advancesToStaffTA_HA_Medicalab))
                .toFixed(2);

            // part 4 //

            ysa.rowYsaData.totalfortbl2a = (ysa
                .parseFloat(ysa.rowYsaData.excsFndinCashbal) + ysa
                .parseFloat(ysa.rowYsaData.clearngEntrsOutstndngforChqsPrsntinClron_thrtyFrst_Mar_Fftn_Prv_wrkdy) + ysa
                .parseFloat(ysa.rowYsaData.clearngDiffrncsPrkdinSndryDepAcnt_Upto_Thrtyth_Sept_Fortn) + ysa
                .parseFloat(ysa.rowYsaData.clearngDiffrncsPrkdinSndryDepAcnt_after_Thrtyth_Sept_Fortn) + ysa
                .parseFloat(ysa.rowYsaData.amntHldinRspctofCnstntsAcntforWntofFullPrtclrs) + ysa
                .parseFloat(ysa.rowYsaData.othersfrtbl2a))
                .toFixed(2);

            // part 5 //

            ysa.rowYsaData.totalfortbl2b = (ysa
                .parseFloat(ysa.rowYsaData.blsRdscntdWthOthrBnks_FIs_IDBI_SIDBI) + ysa
                .parseFloat(ysa.rowYsaData.othrs_If_Any_Pvd))
                .toFixed(2);

            // part 6 //

            ysa.rowYsaData.ttlStf_Advncs = (ysa
                .parseFloat(ysa.rowYsaData.intrstFreStf_Advncs) + ysa
                .parseFloat(ysa.rowYsaData.intrstBerngStf_Advncs))
                .toFixed(2);

            // part 7 //

            ysa.rowYsaData.ttlOutstndng_InlndFrgnabLCCol2Row3 = (ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngInlndLCCol2Row1) + ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngFrgnLCCol2Row2))
                .toFixed(2);

            ysa.rowYsaData.ttlOutstndng_InlndFrgnabGrnteCol4Row3 = (ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngInlndGrnteCol4Row1) + ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngFrgnGrnteCol4Row2))
                .toFixed(2);

            ysa.rowYsaData.ttlOutstndng_InlndFrgnabAcptCol6Row3 = (ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngInlndAcptCol6Row1) + ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngFrgnAcptCol6Row2))
                .toFixed(2);

            ysa.rowYsaData.ttlOutstndng_InlndFrgnabLUCol8Row3 = (ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngInlndLUCol8Row1) + ysa
                .parseFloat(ysa.rowYsaData.ttlOutstndngFrgnLUCol8Row2))
                .toFixed(2);

            ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLCCol2Row8 = (ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplCurntAcntLCCol2Row4) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplTrmDepAcntLCCol2Row5) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinTrmDepAcntLCCol2Row6) + ysa
                .parseFloat(ysa.rowYsaData.lnOnDep_Crnt_Acnt_SB_TDRLCCol2Row7))
                .toFixed(2);

            ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgGrnteCol4Row8 = (ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplCurntAcntGrnteCol4Row4) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplTrmDepAcntGrnteCol4Row5) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinTrmDepAcntGrnteCol4Row6) + ysa
                .parseFloat(ysa.rowYsaData.lnOnDep_Crnt_Acnt_SB_TDRGrnteCol4Row7))
                .toFixed(2);

            ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgAcptCol6Row8 = (ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplCurntAcntAcptCol6Row4) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplTrmDepAcntAcptCol6Row5) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinTrmDepAcntAcptCol6Row6) + ysa
                .parseFloat(ysa.rowYsaData.lnOnDep_Crnt_Acnt_SB_TDRAcptCol6Row7))
                .toFixed(2);

            ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8 = (ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplCurntAcntLUCol8Row4) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinSplTrmDepAcntLUCol8Row5) + ysa
                .parseFloat(ysa.rowYsaData.mrgnMnyHldinTrmDepAcntLUCol8Row6) + ysa
                .parseFloat(ysa.rowYsaData.lnOnDep_Crnt_Acnt_SB_TDRLUCol8Row7))
                .toFixed(2);

            ysa.rowYsaData.ttl_JKLCCol2Row12 = (ysa
                .parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsLCCol2Row10) + ysa
                .parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLCCol2Row11))
                .toFixed(2);

            ysa.rowYsaData.ttl_JKGrnteCol4Row12 = (ysa
                .parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsGrnteCol4Row10) + ysa
                .parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsGrnteCol4Row11))
                .toFixed(2);

            ysa.rowYsaData.ttl_JKAcptCol6Row12 = (ysa
                .parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsAcptCol6Row10) + ysa
                .parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsAcptCol6Row11))
                .toFixed(2);

            ysa.rowYsaData.ttl_JKLUCol8Row12 = (ysa
                .parseFloat(ysa.rowYsaData.onAcntofRupTrnscnsLUCol8Row10) + ysa
                .parseFloat(ysa.rowYsaData.onAcntofFrnCrrcyTrnscnsLUCol8Row11))
                .toFixed(2);


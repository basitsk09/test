            $scope.validateArray = validateArray;

            if (validateArray.length == 0) {

                // alert("flag "+$scope.ysaForm.$dirty);

                if ($scope.ysaForm.$dirty == true) {
                    ysaFactory
                        .submitYSA(copy)
                        .then(function (data) {

                            ysa.displayMessage = null;
                            console.log("result is  flag  ~ Sequence(reportId) ~ reportStatus * " + data);
                            var result = [];
                            result = data.split('~');
                            if (result[0] == 0) {

                                $rootScope.reportObject.reportId = result[1];
                                $rootScope.reportObject.status = result[2];
                                console.log("# reportId " + $rootScope.reportObject.reportId);
                                console.log("# status " + $rootScope.reportObject.status);

                                if (save == true) {
                                    ysa.displayMessage = "Report saved Successfully";
                                    if (ysa.displayMessage) {


                                        $('#myModal4').modal({
                                            backdrop: 'static', keyboard: false, modal: true
                                        });
                                        $('#myModal4').on('shown.bs.modal', function () {
                                            $('#myModal4')
                                                .trigger('focus');
                                        });

                                    }
                                }
                                if (save == false) {
                                    ysa.displayMessage = "Report Submitted Successfully";
                                    if ($rootScope.reportObject.areMocPending == true) {
                                        ysa.displayMessage = "Report was not submitted because of pending MOC.";
                                    }
                                    if (ysa.displayMessage) {

                                        $('#myModal1')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });

                                    }
                                }

                            }

                            // console.log(data);
                        }, function (errResponse) {
                            console
                                .error('Error while submit ysa');
                        });
                }

                if ($scope.ysaForm.$dirty == false) {


                    if (save == false) {
                        ysa.displayMessage111 = "Would you like to submit report without changing data ?";
                    }

                    if (save == true) {
                        ysa.displayMessage111 = "Would you like to save report without changing data ?";
                    }


                    if (ysa.displayMessage111) {

                        $('#myModal33').modal({
                            backdrop: 'static', keyboard: false, modal: true
                        });
                        $('#myModal33').on('shown.bs.modal', function () {
                            $('#myModal33')
                                .trigger('focus');
                        });
                    }

                }

                ysa.YesSubmitYsa = function (rowYsaData) {
                    console.log("************************* MOC from YSA submit " + $rootScope.reportObject.areMocPending + "  ** " + reportObject.areMocPending);
                    var i = {};
                    i = ysa.rowYsaData;
                    console.log(i);
                    var circleCode = $scope.sessionUser.circleCode;
                    var quarterEndDate = $scope.sessionUser.quarterEndDate;
                    var row1 = {
                        'circleCode': circleCode,
                        'quarterEndDate': quarterEndDate,
                        'userId': $scope.sessionUser.userId,
                        'reportName': reportObject.name,
                        'reportId': reportObject.reportId,
                        'reportMasterId': reportObject.reportMasterId,
                        'status': reportObject.status,
                        'areMocPending': $rootScope.reportObject.areMocPending,
                        'save': save
                    };

                    var copy = angular.extend({}, i, row1);

                    ysaFactory
                        .submitYSA(copy)
                        .then(function (data) {
                            ysa.displayMessage = null;

                            console.log("result is  flag  ~ Sequence(reportId) ~ reportStatus * " + data);
                            var result = [];
                            result = data.split('~');
                            if (result[0] == 0) {

                                $rootScope.reportObject.reportId = result[1];
                                $rootScope.reportObject.status = result[2];
                                console.log("# reportId " + $rootScope.reportObject.reportId);
                                console.log("# status " + $rootScope.reportObject.status);

                                if (save == true) {
                                    ysa.displayMessage = "Report saved Successfully";
                                    if (ysa.displayMessage) {


                                        $('#myModal4').modal({
                                            backdrop: 'static', keyboard: false, modal: true
                                        });
                                        $('#myModal4').on('shown.bs.modal', function () {
                                            $('#myModal4')
                                                .trigger('focus');
                                        });

                                    }
                                }

                                if (save == false) {
                                    ysa.displayMessage = "Report Submitted Successfully";
                                    if ($rootScope.reportObject.areMocPending == true) {
                                        ysa.displayMessage = "Report was not submitted because of pending MOC.";
                                    }
                                    if (ysa.displayMessage) {

                                        $('#myModal1')
                                            .modal({
                                                backdrop: 'static', keyboard: false, modal: true
                                            });

                                    }
                                }
                            }

                            //console.log(data);
                        }, function (errResponse) {
                            console
                                .error('Error while submit ysa');
                        });

                }

            } else {
                $('#myModal2').modal({
                    backdrop: 'static', keyboard: false, modal: true
                });

                    });
            }

            if ((parseFloat(ysa.rowYsaData.ttlValueofMrgnMny_Lien_Hld_defgLUCol8Row8) > parseFloat(ysa.rowYsaData.ttlOutstndng_InlndFrgnabLUCol8Row3))) {
                validateArray
                    .push({
                        'ErrorName': "- [H] Total Value of Margin Money/Lien held can not exceed [C] Total Outstanding (Inland + Foreign) for column [LOC]",
